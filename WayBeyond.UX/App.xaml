<Application x:Class="WayBeyond.UX.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WayBeyond.UX"
             xmlns:conv="clr-namespace:WayBeyond.UX.Converters"
             xmlns:behave="http://schemas.microsoft.com/xaml/behaviors"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="FontAwesome">/Fonts/Font Awesome 6 Free Solid.otf#Font Awesome 6 Free Solid</FontFamily>
        <!-- https://fontawesome.com/search?o=r&m=free&s=solid&f=classic -->
        <conv:NegatableBooleanToVisibilityConverter x:Key="VisConverter"/>
        <conv:NegatableBooleanToVisibilityConverter x:Key="NegVisConvert" Negate="True"/>
    </Application.Resources>
    
</Application>
